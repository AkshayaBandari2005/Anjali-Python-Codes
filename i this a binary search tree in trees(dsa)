""" Node is defined as
class node:
  def __init__(self, data):
      self.data = data
      self.left = None
      self.right = None
"""

def check_binary_search_tree_(root):
    res=[]
    def inorder(root):
        if root:
            inorder(root.left)
            res.append(root.data)
            inorder(root.right)
    inorder(root)
    for i in range(len(res)-1):
        if res[i]>=res[i+1]:
            return False
    return True    
       
    
