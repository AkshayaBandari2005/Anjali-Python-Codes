def Fibonacci(n,fibo):
    if n==0 or n==1:
        return fibo[n]
    if fibo[n] == 0:
        fibo[n] = Fibonacci(n-1,fibo) + Fibonacci(n-2,fibo)
    return fibo[n]        

def fibonacci(n):
    if n==0 or n==1:
        return n;
    fibo = [0] * (n+1)
    fibo[0] = 0
    fibo[1] = 1
    return Fibonacci(n,fibo)    

n = int(input())
print(fibonacci(n))
